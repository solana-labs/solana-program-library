  },
  "devDependencies": {
    "chai": "^4.3.7",
    "dir-compare": "^4.0.0",
    "eslint": "^8.32.0",
    "eslint-config-tschaub": "^14.1.2",
    "mocha": "^10.2.0",
    "sinon": "^15.0.1",
    "tmp": "^0.2.1"
  },
  "bin": {
    "gh-pages": "bin/gh-pages.js",
    "gh-pages-clean": "bin/gh-pages-clean.js"
