    "buffer-layout": "^1.2.0",
    "dotenv": "8.2.0",
    "json-to-pretty-yaml": "^1.2.2",
    "mkdirp-promise": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
