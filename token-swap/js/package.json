{
  "name": "@stepfinance/step-swap",
  "version": "0.1.7",
  "description": "Step Token Swap JavaScript API",
  "license": "MIT",
  "author": "Step <dev@step.finance>",
  "homepage": "https://step.finance/",
  "repository": {
    "type": "git",
    "url": "https://github.com/step-finance/solana-program-library"
  },
  "bugs": {
    "url": "https://github.com/step-finance/solana-program-library/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "browser": "dist.browser/index.js",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "files": [
    "/dist",
    "/dist.browser"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.browser.json && npm run generate:flow",
    "prepare": "npm run build",
    "start": "ts-node cli/main.ts",
    "start-with-test-validator": "start-server-and-test 'solana-test-validator --bpf-program SSwpMgqNDsyV7mAgN9ady4bDVu5ySjmmXejXvy2vLt1 ../../target/deploy/spl_token_swap.so --reset --quiet' http://localhost:8899/health start",
    "lint": "npm run pretty && eslint .",
    "lint:fix": "npm run pretty:fix && eslint . --fix",
    "generate:flow": "flowgen dist/index.d.ts -o dist/module.flow.js",
    "lint:watch": "watch 'npm run lint:fix' . --wait=1",
    "build:program": "cargo build-bpf --manifest-path ../program/Cargo.toml",
    "cluster:localnet": "rm -f .env",
    "cluster:devnet": "cp cluster-devnet.env .env",
    "cluster:testnet": "cp cluster-testnet.env .env",
    "cluster:mainnet-beta": "cp cluster-mainnet-beta.env .env",
    "pretty": "prettier --check '{,???/**/}*.ts'",
    "pretty:fix": "prettier --write '{,???/**/}*.ts'"
  },
  "keywords": [],
  "dependencies": {
    "@solana/web3.js": "^1.22.0",
    "bn.js": "^5.1.3",
    "buffer-layout": "^1.2.0",
    "dotenv": "10.0.0",
    "json-to-pretty-yaml": "^1.2.2",
    "mkdirp": "1.0.4"
  },
  "devDependencies": {
    "@solana/spl-token": "0.1.6",
    "@types/bn.js": "^5.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.23.4",
    "flow-bin": "0.157.0",
    "flow-typed": "^3.3.1",
    "flowgen": "^1.14.1",
    "mz": "^2.7.0",
    "prettier": "^2.3.2",
    "start-server-and-test": "^1.13.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "watch": "^1.0.2"
  },
  "engines": {
    "node": ">= 10"
  }
}
